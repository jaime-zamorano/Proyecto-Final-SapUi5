{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run ProyectoFinal-RecursoHumano",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/proyectofinal-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/EmpleadosRecursoHumano/index.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/RecursoHumano",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/ProyectoFinal/RecursoHumano\"}"
      },
      "envFile": "${workspaceFolder}/RecursoHumano/.env1",
      "preLaunchTask": "Build ProyectoFinal"
    }
  ]
}